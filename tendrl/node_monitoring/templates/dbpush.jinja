LoadPlugin "write_graphite"

<Plugin write_graphite>
    <Node "{{ hostname }}">
        Host "{{ time_series_db_server }}"
        Port "2003"
        Protocol "tcp"
        LogSendErrors true
        Prefix "collectd."
        StoreRates true
        AlwaysAppendDS false
        EscapeCharacter "_"
    </Node>
</Plugin>

